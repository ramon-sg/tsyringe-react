    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "dts-cli": "^1.6.0",
    "husky": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "size-limit": "^8.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "tsyringe": "^4.7.0"
